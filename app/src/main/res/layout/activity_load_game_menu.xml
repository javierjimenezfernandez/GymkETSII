<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".LoadGameMenu">

    <fragment
        android:id="@+id/fragment_load_game_menu_upper"
        android:name="com.gymketsii.LoadGameMenuUpperFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_load_game_menu_upper"/>

    <fragment
        android:id="@+id/fragment_load_game_menu_lower"
        android:name="com.gymketsii.LoadGameMenuLowerFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_load_game_menu_lower"/>

</LinearLayout>